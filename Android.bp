java_import {
    name: "boringSharedSystemUI",

    jars: ["app/libs/impl/sysui_shared.jar"],
    installable: false,
    sdk_version: "current",
}

java_import {
    name: "boringSystemUIPluginLib",

    jars: ["app/libs/compile/SystemUIPluginLib.jar"],
    installable: false,
    sdk_version: "current",
}

android_app {
    name: "BoringdroidSystemUI",

    libs: ["boringSystemUIPluginLib"],
    static_libs: [
        "boringSharedSystemUI",
        "androidx.recyclerview_recyclerview",
    ],

    certificate: "platform",
    optimize: {
        enabled: false,
    },

    resource_dirs: ["app/src/main/res"],
    srcs: ["app/src/main/**/*.java"],
}
